#include <stdio.h>
 
int main() {
    int numeros[10];
    for (int i = 0; i < 10; i++)
    {
        int num;
        scanf("%d", &num);
        if (num <= 0)
        {
            num = 1;
        }
        numeros[i] = num;
        
    }
    for (int i = 0; i < 10; i++)
        {
            printf("X[%d] = %d\n",i,numeros[i]);
        }
    return 0;
}
